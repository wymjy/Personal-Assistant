                android:layout_centerVertical="true"
                android:layout_marginLeft="20dip"
                android:text="分享"/>

        </RelativeLayout>




        <ImageView
            android:id="@+id/image13_ce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
