        <RelativeLayout
            android:id="@+id/relative1_ce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dip">



            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/head"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:src="@drawable/touxiang"
                android:layout_marginLeft="20dip"
                android:layout_centerVertical="true"/>

            <Button
                android:id="@+id/button_close"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/head"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dip"
                android:background="@drawable/back_button1_selector" />

        </RelativeLayout>

        <ImageView
            android:id="@+id/image0_ce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/jiangetiao_ce"
            android:layout_marginTop="20dip"
            android:layout_marginLeft="7dip"
            android:layout_marginRight="7dip"
            android:layout_centerHorizontal="true"
            android:layout_below="@id/relative1_ce"/>

        <RelativeLayout
            android:id="@+id/relative4_ce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@id/image0_ce">

            <Button
