            <TextView
                android:id="@+id/jiange3_ce"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/image6_ce"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dip"
                android:text="日程管理"/>

        </RelativeLayout>

        <ImageView
            android:id="@+id/image7_ce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/jiangetiao_ce"
            android:layout_marginLeft="7dip"
            android:layout_centerHorizontal="true"
            android:layout_below="@id/relative4_ce"/>

        <RelativeLayout
            android:id="@+id/relative5_ce"
